YearRelease(row.item.release_date)}}\n                </td>\n            </tr>\n        </template>\n    </v-data-table>\n  </v-card>\n</template>\n\n<script>\nimport { format } from 'date-fns'\n  export default {\n    name: 'AlbumsComponent',\n    computed:{\n      albums(){\n        console.log(this.$store.state.discography.albums)\n        return this.$store.state.discography.albums\n      },\n    },\n    data:()=>{\n        return {\n          search: '',\n      }\n    },\n    methods:{\n      returnYearRelease(dateTime){\n        return format(new Date(dateTime), \"yyyy\")\n      },\n      detailAlbum(row){\n        return row;\n      }\n    }\n  }\n</script>\n"]},"metadata":{},"sourceType":"module"}